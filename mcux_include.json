{
  "version": 7,
  "cmakeMinimumRequired": {
    "major": 3
  },
  "configurePresets": [
    {
      "name": "debug-env",
      "displayName": "debug-env",
      "hidden": true,
      "environment": {
        "ZEPHYR_BASE": "$env{ZEPHYR_BASE}",
        "ZEPHYR_SDK_INSTALL_DIR": "$env{ZEPHYR_SDK_INSTALL_DIR}",
        "MCUX_VENV_PATH": "$env{MCUX_VENV_PATH}",
        "PATH": "$env{MCUX_VENV_PATH}${pathListSep}$penv{PATH}"
      },
      "cacheVariables": {}
    },
    {
      "name": "release-env",
      "displayName": "release-env",
      "hidden": true,
      "environment": {
        "ZEPHYR_BASE": "$env{ZEPHYR_BASE}",
        "ZEPHYR_SDK_INSTALL_DIR": "$env{ZEPHYR_SDK_INSTALL_DIR}",
        "MCUX_VENV_PATH": "$env{MCUX_VENV_PATH}",
        "PATH": "$env{MCUX_VENV_PATH}${pathListSep}$penv{PATH}"
      },
      "cacheVariables": {}
    }
  ],
  "buildPresets": []
}
